#
# Debian - Generated by Chef
#
<% @repos.each do | repo | -%>

deb <%= repo["url"] %> <%= repo.has_key?('codename') ? repo["codename"] : @code_name -%>
<% sections = repo.has_key?('sections') ? repo["sections"] : @sections -%>
<%  sections.each do | section | -%> <%= section %> <% end %>
deb-src <%= repo['url'] %> <%= repo.has_key?('codename') ? repo['codename'] : @code_name -%> 
<% sections = repo.has_key?('sections') ? repo["sections"] : @sections -%>
<%  sections.each do | section | -%> <%= section %> <% end %>
<% end %>
